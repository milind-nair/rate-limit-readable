{
  "compilerOptions": {
    "target": "ES2019",
    "moduleResolution": "Node",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "declaration": false,
    "rootDir": "src",
    "isolatedModules": true
  },
  "include": ["src"]
}
